{% extends 'base.html.twig' %}

{% block title %}SubscriptionHistory{% endblock %}

{% block body %}
    <h1>SubscriptionHistory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ subscription_history.id }}</td>
            </tr>
            <tr>
                <th>StartAt</th>
                <td>{{ subscription_history.startAt ? subscription_history.startAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>EndAt</th>
                <td>{{ subscription_history.endAt ? subscription_history.endAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_subscription_history_index') }}">back to list</a>

    <a href="{{ path('app_subscription_history_edit', {'id': subscription_history.id}) }}">edit</a>

    {{ include('subscription_history/_delete_form.html.twig') }}
{% endblock %}
